<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.life.dao.CustomMadeDao">

	<resultMap id="customMade" type="com.life.model.CustomMadeModel">
		<result column="id" property="id" jdbcType="VARCHAR" />
		<result column="usercode" property="usercode" jdbcType="VARCHAR" />
		<result column="filename" property="filename" jdbcType="VARCHAR" />
		<result column="filetype" property="filetype" jdbcType="VARCHAR" />
		<result column="filesize" property="filesize" jdbcType="VARCHAR" />
		<result column="createtime" property="createtime" jdbcType="VARCHAR" />
		<result column="filepath" property="filepath" jdbcType="VARCHAR" />
		<result column="fileurl" property="fileurl" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="columns">
		id,usercode,filename,filetype,filesize,createtime,filepath,fileurl
	</sql>
	<select id="getListsByCode" parameterType="java.lang.String" resultMap="customMade">
		select
		<include refid="columns" />
		from custom_made where usercode =#{code,jdbcType=VARCHAR}
	</select>

	<update id="update" parameterType="com.life.model.CustomMadeModel">
		update custom_made
		<set>
			<if test="id!=null">
				id=#{id,jdbcType=VARCHAR},
			</if>
			<if test="userCode!=null">
				userCode=#{userCode,jdbcType=VARCHAR},
			</if>
			<if test="filename!=null">
				filename=#{filename,jdbcType=VARCHAR},
			</if>
			<if test="filetype!=null">
				filetype=#{filetype,jdbcType=VARCHAR},
			</if>
			<if test="filesize!=null">
				filesize=#{filesize,jdbcType=VARCHAR},
			</if>
			<if test="createtime!=null">
				createtime=#{createtime,jdbcType=VARCHAR},
			</if>
			<if test="filepath!=null">
				filepath=#{filepath,jdbcType=VARCHAR},
			</if>
			<if test="fileurl!=null">
				fileurl=#{fileurl,jdbcType=VARCHAR},
			</if>
		</set>
		where usercode =#{usercode,jdbcType=VARCHAR}
	</update>

	<insert id="add" parameterType="com.life.model.CustomMadeModel">
		insert into custom_made
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id!=null">
				id,
			</if>
			<if test="userCode!=null">
				usercode,
			</if>
			<if test="filename!=null">
				filename,
			</if>
			<if test="filetype!=null">
				filetype,
			</if>
			<if test="filesize!=null">
				filesize,
			</if>
			<if test="createtime!=null">
				createtime,
			</if>
			<if test="filepath!=null">
				filepath,
			</if>
			<if test="fileurl!=null">
				fileurl,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id!=null">
				#{id,jdbcType=VARCHAR},
			</if>
			<if test="userCode!=null">
				#{userCode,jdbcType=VARCHAR},
			</if>
			<if test="filename!=null">
				#{filename,jdbcType=VARCHAR},
			</if>
			<if test="filetype!=null">
				#{filetype,jdbcType=VARCHAR},
			</if>
			<if test="filesize!=null">
